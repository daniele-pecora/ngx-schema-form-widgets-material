{
  "name": "ngx-schema-form-widgets-material-app",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl true --host 0.0.0.0 --port 4321",
    "build": "ng build",
    "test": "ng test",
    "test:chrome_headless": "ng test --browsers=ChromeHeadless --code-coverage --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build --prod ngx-schema-form-widgets-material",
    "pack:lib": "npm pack dist/ngx-schema-form-widgets-material/",
    "pre-publish:lib": "cp -f README.MD dist/ngx-schema-form-widgets-material/README.MD",
    "publish:lib": "npm run pre-publish:lib && npm publish dist/ngx-schema-form-widgets-material/",
    "deploy:lib": "npm run build:lib && npm run pack:lib && npm run publish:lib",
    "start:lib": "npm run build:lib && npm start",
    "-- NUCLI --": "-- ------ --",
    "styles:watch": "sass --watch --no-source-map src/assets",
    "styles:build": "sass --no-source-map src/assets",
    "build:prod": "ng build -c=production --prod --aot --sourceMap=false --build-optimizer",
    "-- Electron --": "-- ------ --",
    "electron:package.json": "node -e \"var fs = require('fs');fs.writeFileSync('dist/ngx-schema-form-widgets-material-app/package.json', fs.readFileSync('electron-package.json'));\"",
    "electron:main.js": "node -e \"var fs = require('fs');fs.writeFileSync('dist/ngx-schema-form-widgets-material-app/electron-main.js', fs.readFileSync('electron-main.js'));\"",
    "electron:pre-build": "npm run electron:package.json && npm run electron:main.js",
    "electron:build": "ng build --base-href ./ && npm run electron:pre-build",
    "electron:start": "electron dist/ngx-schema-form-widgets-material-app",
    "electron:open": "npm run electron:pre-build && npm run electron:start",
    "electron:run": "npm run electron:build && npm run electron:start",
    "bundle-report": "ng build --prod --stats-json --vendor-chunk true && webpack-bundle-analyzer dist/ngx-schema-form-widgets-material-app/stats.json",
    "bundle-report:open": "webpack-bundle-analyzer dist/ngx-schema-form-widgets-material-app/stats.json",
    "doc": "typedoc --hideGenerator --target ES5 --excludeExternals --includeDeclarations --readme README.MD --name ngx-schema-form-widgets-material --module commonjs --out doc",
    "--": "--",
    "build:prod-server": "npm run styles:build && ng build --prod -c=production --aot --sourceMap=false --build-optimizer --base-href ./",
    "electron:windows": "npm run electron:build && electron-packager dist/ngx-schema-form-widgets-material-app --platform=win32",
    "electron:darwin": "npm run electron:build && electron-packager dist/ngx-schema-form-widgets-material-app --platform=darwin",
    "electron:linux": "npm run electron:build && electron-packager dist/ngx-schema-form-widgets-material-app --platform=linux",
    "electron:mac": "npm run electron:build && electron-packager dist/ngx-schema-form-widgets-material-app --platform=mac",
    "-- pre-build --": "",
    "prebuild": "node update-tsconfig.app.json-files.js",
    "prestart": "node update-tsconfig.app.json-files.js",
    "prebuild:prod-server": "node update-tsconfig.app.json-files.js",
    "prebuild:prod": "node update-tsconfig.app.json-files.js",
    "preelectron:build": "node update-tsconfig.app.json-files.js",
    "prebundle-report": "node update-tsconfig.app.json-files.js",
    "-- Firebase deploy --": "",
    "firebase:publish": "npm run build:prod-server && firebase deploy",
    "-- Copy resources --": "",
    "copy:css-resources": "node -e \"var src=['./projects/ngx-schema-form-widgets-material/src/ngx-schema-form-widgets-material.css','./projects/ngx-schema-form-widgets-material/src/primeng-ui-g.css']; var dest=['./dist/ngx-schema-form-widgets-material/ngx-schema-form-widgets-material.css','./dist/ngx-schema-form-widgets-material/primeng-ui-g.css']; var fs = require('fs'); for(var i=0;i<src.length;i++){if (fs.existsSync(src[i])) { var data = fs.readFileSync(src[i], 'utf-8'); fs.writeFileSync(dest[i], data);}}\"",
    "prepack:lib": "npm run copy:css-resources",
    "postinstall": "npm run styles:build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.11",
    "@angular/cdk": "9.2.4",
    "@angular/common": "9.1.11",
    "@angular/compiler": "9.1.11",
    "@angular/core": "9.1.11",
    "@angular/forms": "9.1.11",
    "@angular/localize": "^9.1.11",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "9.1.11",
    "@angular/platform-browser-dynamic": "9.1.11",
    "@angular/router": "9.1.11",
    "@techiediaries/ngx-qrcode": "^9.1.0",
    "angular2-text-mask": "^9.0.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "jexl": "^2.3.0",
    "moment": "2.20.1",
    "ngx-openlayers": "^1.0.0-next.15",
    "ngx-recaptcha-easy": "^9.0.0",
    "ngx-schema-form": "~2.5.20",
    "ngx-schema-form-view": "~1.0.10",
    "ol": "^6.3.1",
    "quill": "1.3.7",
    "rxjs": "^6.5.5",
    "sw-precache": "^5.2.1",
    "tslib": "^1.10.0",
    "web-animations-js": "2.3.2",
    "z-schema": "^4.2.3",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.12",
    "@angular-devkit/build-ng-packagr": "^0.901.8",
    "@angular/cli": "9.1.8",
    "@angular/compiler-cli": "9.1.11",
    "@angular/language-service": "9.1.11",
    "@types/jasmine": "3.3.13",
    "@types/jasminewd2": "2.0.6",
    "@types/jexl": "^2.1.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "electron": "^11.5.0",
    "electron-packager": "^12.2.0",
    "enhanced-resolve": "^3.4.1",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "6.4.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "ng-packagr": "^10.1.1",
    "protractor": "5.4.2",
    "puppeteer": "5.2.1",
    "sass": "^1.38.0",
    "sass-loader": "^12.1.0",
    "ts-node": "7.0.1",
    "tslint": "5.17.0",
    "typedoc": "^0.8.0",
    "typescript": "^3.7.5",
    "typescript-require": "^0.3.0",
    "webpack": "^4.39.1",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
